{
  "name": "gatsby-shopify-example",
  "description": "Example Gatsby website to test Shopify static and live data",
  "version": "0.1.0",
  "author": "Angelo Ashmore",
  "scripts": {
    "format": "prettier --write README.md 'src/**/*.js' 'gatsby-*.js'",
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "@walltowall/gatsby-theme-ww-base": "^0.5.2",
    "@walltowall/system": "^1.0.1",
    "formik": "^1.3.1",
    "gatsby": "^2.0.77",
    "gatsby-awesome-pagination": "^0.3.3",
    "gatsby-plugin-create-client-paths": "^2.0.3",
    "gatsby-plugin-root-import": "^2.0.4",
    "gatsby-source-shopify": "^2.0.8",
    "inter-ui": "^3.0.0",
    "lodash": "^4.17.11",
    "minireset.css": "^0.0.3",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-helmet": "^5.2.0",
    "react-shopify-hooks": "^0.2.1",
    "styled-components": "^4.1.3",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "husky": "1.3.1",
    "prettier": "^1.15.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
